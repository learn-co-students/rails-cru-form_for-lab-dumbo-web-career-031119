<%= form_for @song do |f|%>
<%= label_tag 'song[name]','song name'%>
<%= text_field_tag 'song[name]',"#{@song.name}"%>
<%= label_tag 'song[genre_id]','song genre_id'%>
<%= text_area_tag 'song[genre_id]',"#{@song.genre_id}"%>
<%= label_tag 'song[artist_id]','song artist_id'%>
<%= text_area_tag 'song[artist_id]',"#{@song.artist_id}"%>
<%= submit_tag 'Update Song'%>
<%end%>
